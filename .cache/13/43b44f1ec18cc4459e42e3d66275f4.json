n":0}},{"source":"src/components/HeroesForm.vue","name":null,"original":{"line":56,"column":0},"generated":{"line":91,"column":0}},{"source":"src/components/HeroesForm.vue","name":null,"original":{"line":44,"column":0},"generated":{"line":92,"column":0}}],"sources":{"src/components/HeroesForm.vue":"<template>\n  <div class=\"row\">\n    <div class=\"col-6 offset-3 mt-3\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h5 class=\"card-title\">Choose your heroes</h5>\n          <form @submit.prevent=\"addHeroes\">\n            <div class=\"form-group row\">\n              <label for=\"name\" class=\"col-sm-4 col-form-label\">Heroes Name</label>\n              <div class=\"col-sm-8\">\n                <input type=\"text\" class=\"form-control\" id=\"name\" v-model=\"form.name\" required>\n              </div>\n            </div>\n            <div class=\"form-group row\">\n              <label for=\"type\" class=\"col-sm-4 col-form-label\">Type</label>\n              <div class=\"col\">\n                <select id=\"type\" class=\"form-control\" v-model=\"form.type\" required>\n                  <option value=\"\" disabled>Please select one</option>\n                  <option :value=\"type.name\" v-for=\"type in types\" :key=\"type.id\">{{ type.name }}</option>\n                </select>\n              </div>\n            </div>\n            <div class=\"form-group row\">\n              <label for=\"image\" class=\"col-sm-4 col-form-label\">Image</label>\n              <div class=\"col-sm-8\">\n                <input type=\"text\" class=\"form-control\" id=\"image\" v-model=\"form.image\" required>\n              </div>\n            </div>\n            <div class=\"form-group row\">\n              <div class=\"col\">\n                <input type=\"submit\" class=\"btn btn-danger\" value=\"Pick Heroes\">\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: \"HeroesForm\",\n  data () {\n    return {\n      form: {\n        name: '',\n        type: '',\n        image: ''\n      }\n    }\n  },\n  props :['types'],\n  methods: {\n    addHeroes () {\n      axios({\n        method: \"POST\",\n        url: \"http://localhost:3000/heroes\",\n        data : {\n          name: this.form.name,\n          type: this.form.type,\n          imageURL: this.form.image\n        }\n      })\n        .then(({data}) => {\n          this.clearForm();\n          this.$emit('refetchHeroes');\n        })\n        .catch(err => {\n          console.log(err);\n        })\n    },\n    clearForm () {\n      this.form.name = '';\n      this.form.type = '';\n      this.form.image = '';\n    }\n  }\n\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"772b5604f1c717bf07893a139fa89fb7","cacheData":{"env":{}}}